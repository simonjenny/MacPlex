{
  "name": "macplex",
  "version": "1.1.2",
  "description": "MacPlex - A alternative Plex App for Mac",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build-local": "node_modules/.bin/electron-builder -l"
  },
  "author": "Simon Jenny <simon.jenny@me.com>",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "latest",
    "electron-builder": "latest"
  },
  "build": {
    "productName": "MacPlex",
    "appId": "dev.simonjenny.MacPlex",
    "mac": {
      "darkModeSupport": true,
      "target": "dmg"
    },
    "linux": {
      "target": "deb"
    },
    "win": {
      "target": "nsis"
    }
  },
  "dependencies": {
    "electron-aspectratio": ">=1.0.1",
    "electron-positioner": ">=4.1.0",
    "electron-window-state": "^5.0.3",
    "is-reachable": "^4.0.0"
  }
}
